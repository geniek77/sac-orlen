{
    "id": "com.orlen.sap.orlencircleprogressbarwidget",
    "version": "1.0.0",
    "name": "Orlen Circle Progress Bar Widget",
    "description": "Orlen circle progress bar",
    "newInstancePrefix": "OrlenCircleProgressBarWidget",
    "vendor": "G++ E.Sz dla Orlen S.A",
    "eula": "",
    "license": "",
    "icon": "",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-orlen-sap-orlencircleprogressbarwidget",
            "url": "/OrlenCircleProgressBarWidget.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-orlen-sap-orlencircleprogressbarwidget-builder",
            "url": "/OrlenCircleProgressBarWidget_Builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "percentage": {
            "description": "Percentage",
            "type": "integer",
            "default": 75
        },
        "barColor": {
            "description": "Bar Color",
            "type": "string",
            "default": "#03ff4f"
        },
        "emptyBarColor": {
            "description": "Empty Bar Color",
            "type": "string",
            "default": "#ededed"
        }
    },
    "methods": {
        "setPercentage": {
            "description": "Set Percentage",
            "parameters": [
                {
                    "name": "percentage",
                    "type": "integer",
                    "description": "Percentage"
                }
            ],
            "body": "this._props=this._props||{}; this._props.percentage=percentage; this.dispatchEvent(new CustomEvent('propertiesChanged',{detail:{properties:{percentage:percentage}}}));"
            
        },
        "getPercentage": {
            "returnType": "integer",
            "description": "Return Percentage",
            "body": "return (this._props && this._props.percentage != null) ? this._props.percentage : 0;"

        },
        "setBarColor": {
            "description": "Set Bar Color",
            "parameters": [
                {
                    "name": "barColor",
                    "type": "string",
                    "description": "Bar Color"
                }
            ],
            "body": "this._props=this._props||{}; this._props.barColor=barColor; this.dispatchEvent(new CustomEvent('propertiesChanged',{detail:{properties:{barColor:barColor}}}));"

        },
        "getBarColor": {
            "returnType": "string",
            "description": "Return Bar Color",
            "body": "return (this._props && this._props.barColor != null) ? this._props.barColor : '';"
        },
        "setEmptyBarColor": {
            "description": "Set Empty Bar Color",
            "parameters": [
                {
                    "name": "emptyBarColor",
                    "type": "string",
                    "description": "Empty Bar Color"
                }
            ],
            "body": "this._props=this._props||{}; this._props.emptyBarColor=emptyBarColor; this.dispatchEvent(new CustomEvent('propertiesChanged',{detail:{properties:{emptyBarColor:emptyBarColor}}}));"

        },
        "getEmptyBarColor": {
            "returnType": "string",
            "description": "Return Empty Bar Color",
            "body": "return (this._props && this._props.emptyBarColor != null) ? this._props.emptyBarColor : '';"
        }
    },
    "events": {
        "onClick": {
            "description": "User Clicked."
        }
    }
}